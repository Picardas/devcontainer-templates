{
    "id": "ubi-minimal",
    "version": "1.0.0",
    "name": "UBI Minimal",
    "description": "Minimal Red Hat Universal Base Image(UBI) container with Git installed.",
    "documentationURL": "https://github.com/Picardas/devcontainer-templates/tree/main/src/ubi-minimal",
    "publisher": "Picardas",
    "licenseURL": "https://github.com/Picardas/devcontainer-templates/blob/main/LICENSE",
    "options": {
        "imageVariant": {
            "type": "string",
            "description": "Red Hat UBI version:",
            "proposals": [
                "8",
                "9"
            ],
            "default": "9"
        }
    },
    "platforms": ["Any"],
    "optionalPaths": [
        ".github/*"
    ]
}
